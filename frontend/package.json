{
  "name": "birdybop-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 8080 --clearScreen false",
    "build": "vue-tsc --noEmit && vite build --mode production",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom --run",
    "test:unit:watch": "vitest --environment jsdom --watch",
    "typecheck": "vue-tsc --noEmit && vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --no-fix",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "deploy": "aws --profile keeshu-deployer s3 sync ./dist s3://www.birdybop.com --delete"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "dompurify": "^2.3.6",
    "highlight.js": "^11.4.0",
    "lodash": "^4.17.21",
    "marked": "^4.0.12",
    "pinia": "^2.0.11",
    "register-service-worker": "^1.7.1",
    "simpledotcss": "^2.0.1",
    "vue": "^3.2.29",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/chai": "^4.3.0",
    "@types/dompurify": "^2.3.3",
    "@types/jsdom": "^16.2.14",
    "@types/lodash": "^4.14.178",
    "@types/marked": "^4.0.2",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.11.22",
    "@vitejs/plugin-vue": "^2.1.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "jsdom": "^19.0.0",
    "sass": "^1.49.7",
    "typescript": "~4.5.5",
    "vite": "^2.7.13",
    "vitest": "^0.2.5",
    "vue-tsc": "^0.31.1"
  }
}
